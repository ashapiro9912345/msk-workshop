{
  "name": "redshift-jdbc-sink",
  "config": {
    "connector.class": "io.confluent.connect.jdbc.JdbcSinkConnector",
    "tasks.max": "3",
    "topics": "<your_topics>",
    "connection.url": "jdbc:redshift://<REDSHIFT_ENDPOINT>:5439/<db>?user=<user>&password=<password>",
    "auto.create": "true",
    "insert.mode": "upsert",
    "pk.fields": "id",
    "pk.mode": "record_key"
  }
}
